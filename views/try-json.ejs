<%- include('parts/html-head') %> <%- include('parts/navbar') %>

<div class="container">
    <table class="table table-striped">
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">Name</th>
                <th scope="col">Age</th>
            </tr>
        </thead>
        <tbody>
            <!--後端資料生成頁面-->
            <% for(let item of rows){ %>
            <tr>
                <td><%= item.id %></td>
                <td><%= item.name %></td>
                <td><%= item.age %></td>
            </tr>
            <% } %>
        </tbody>
    </table>
</div>

<%- include('parts/scripts') %> 
<script>
    //前端拿後端數據
    const rows = <%- JSON.stringify(rows) %>;

</script>
<%- include('parts/html-foot') %>
